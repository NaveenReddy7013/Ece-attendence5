<!DOCTYPE html>
<html>
<head>
    <title>Date Input Form</title>
</head>
<body>
    <h2>Date Input Form</h2>
    <form action="#" method="post">
        <label for="date">Select a Date:</label>
        <input type="date" name="date" value="Submit">
        <br><br>
        <input type="submit" value="Submit">
    </form>
    <script>
        const scriptURL = 'https://script.google.com/macros/s/AKfycbwMylRxW8wnRonDMo-DoaU8fQYU2PQQH1s4LuME3I0QadMnI0NffZsM29QJRxpwkHYD/exec'
        const form = document.forms['submit-to-google-sheet']
        const success=document.getElementById('success');
      
        form.addEventListener('submit', e => {
          e.preventDefault()
          fetch(scriptURL, { method: 'POST', body: new FormData(form)})
            .then(response =>{
                success.innerHTML="data successfully submitted";
                setTimeout(function()
                {
                    success.innerHTML="";
                },5000)
                form.reset()
            })
            .catch(error => console.error('Error!', error.message))
        })
      </script>
</body>
</html>






























































<script>
